# Cumulus Labs VLM Configuration
CUMULUS_VLM_ENDPOINT=http://your-vlm-endpoint:8000/v1
CUMULUS_VLM_MODEL=Qwen/Qwen3-VL-32B-Instruct-FP8
CUMULUS_VLM_API_KEY=your-api-key

# DaVinci Resolve SDK paths (macOS defaults)
RESOLVE_SCRIPT_API=/Library/Application Support/Blackmagic Design/DaVinci Resolve/Developer/Scripting
RESOLVE_SCRIPT_LIB=/Applications/DaVinci Resolve/DaVinci Resolve.app/Contents/Libraries/Fusion/fusionscript.so

# Supabase OAuth
MCP_USE_OAUTH_SUPABASE_PROJECT_ID=your-supabase-project-id
MCP_USE_OAUTH_SUPABASE_JWT_SECRET=your-jwt-secret

# Server
PORT=3000
